/* pages/homeIndex/homeIndex.wxss */

.page {
    height: 100vh;
    width: 100vw;
    min-height: 100vh;
    background-size: 100vw 408rpx;
    background-repeat: no-repeat;
    background-color: #F9F9F9;
}

.p-0 {
    padding: 0!important;
}

.top-bg {
    position: relative;
    height: 408rpx;
}

button {
    background: transparent;
    padding: 0;
}

.cart {
    position: relative;
    top: -10rpx;
    right: 40rpx;
    /* transform: translateY(-50%); */
    z-index: 9999;
}

.cart image {
    width: 120rpx;
    height: 120rpx;
}

.cart .num {
    width: 30rpx;
    height: 30rpx;
    background: #CF2525;
    color: #fff;
    text-align: center;
    line-height: 30rpx;
    font-size: 20rpx;
    position: absolute;
    top: 20rpx;
    right: 20rpx;
    border-radius: 50%;
}

.adv {
    padding: 14rpx 30rpx;
    background: #00000080;
    color: #2D879C;
    font-size: 24rpx;
    line-height: 24rpx;
    border-radius: 26rpx!important;
    width: 690rpx;
    margin-left: 30rpx;
}

.adv image {
    width: 18rpx;
    height: 20rpx
}

.swiper {
    width: 750rpx;
    position: absolute;
    left: 0;
    top: 20rpx;
    padding: 0 30rpx;
    border-radius: 16rpx;
    overflow: hidden;
    transform: translateY(-40rpx);
}

.swiper .img {
    border-radius: 16rpx;
    overflow: hidden;
    width: 690rpx!important;
}

.container {
    display: flex;
    flex-wrap: wrap;
}

.container image {
    width: 100%;
    height: 100%;
}

.shop {
    height: 136rpx;
    position: relative;
    background-color: rgba(249, 249, 249, 1);
    padding-top: 98rpx;
}

.shop .coupon {
    width: 690rpx;
    height: 136rpx;
    position: absolute;
    top: -68rpx;
    left: 50%;
    transform: translateX(-50%);
    background: #ffffff;
    border-radius: 24rpx;
    padding: 24rpx 30rpx;
}

.shop .photo {
    width: 88rpx;
    height: 88rpx;
    border-radius: 50%;
    margin-right: 20rpx;
}

.shop .photo .name {
    font-size: 28rpx;
}

.shop .photo image {
    border-radius: 50%;
}

.shop .couponBtn {
    width: 180rpx;
    height: 72rpx;
    line-height: 72rpx;
    background-color: rgba(45, 135, 156, 1);
    border-radius: 36rpx;
    font-size: 28rpx;
    color: #ffffff;
    padding: 0 20rpx;
    text-align: center;
}

.shop image {
    width: 100%;
}

.shop .left {
    width: 330rpx;
    height: 100%;
    margin-right: 30rpx;
}

.shop .right {
    width: 330rpx;
    height: 100%;
}

.shop .right .topBox,
.shop .right .bottomBox {
    width: 330rpx;
    height: 150rpx;
}

.shop .right .topBox {
    margin-bottom: 30rpx;
}

.caffeeImg {
    width: 30rpx!important;
    height: 30rpx!important;
    margin-right: 10rpx;
    margin-bottom: 20rpx;
}

.caffeeTitle {
    font-size: 32rpx;
    font-weight: 600;
    margin-bottom: 30rpx;
    margin-top: 10rpx
}

.mr {
    margin-right: 30rpx;
}

swiper,
.modot {
    /* height: 320rpx */
}

.modot {
    position: relative;
    padding: 20rpx 40rpx;
}

.modot .btn {
    width: 336rpx;
    height: 70rpx;
    background: rgba(37, 87, 167, 0.5);
    color: #fff;
    font-size: 28rpx;
    line-height: 70rpx;
    font-weight: bold;
    border-radius: 35rpx;
    text-align: center;
    position: absolute;
    top: 198rpx;
    left: 42rpx;
}

.modot .word {
    font-size: 24rpx;
    line-height: 20rpx;
    position: absolute;
    top: 298rpx;
    left: 50rpx;
}

.modot text {
    color: rgba(208, 13, 13, 1);
    font-weight: bold
}

.modot .num {
    position: absolute;
    top: 28rpx;
    left: 314rpx;
    font-size: 30rpx
}

.modot .detail {
    position: absolute;
    top: 28rpx;
    left: 458rpx;
    width: 180rpx;
    height: 64rpx
}

.modot .bignum {
    font-size: 60rpx;
    color: #FF8400;
    line-height: 60rpx
}

.modot .auth {
    position: absolute;
    top: 138rpx;
    left: 36rpx;
    width: 428rpx;
    height: 170rpx;
    display: flex;
    justify-content: space-between;
    flex-direction: row;
    flex-wrap: wrap;
}

.modot .auth .picture {
    width: 70rpx;
    height: 70rpx;
    margin-right: 10rpx;
}

.article {
    width: 690rpx;
    background: #ffffff;
    padding: 30rpx;
    border-radius: 24rpx;
    margin-bottom: 20rpx;
}

.article .top {
    height: 290rpx;
    border-radius: 20rpx;
    overflow: hidden
}

.article .bottom {
    height: 100%;
    position: relative
}

.article .bottom .title {
    font-size: 28rpx;
    margin-bottom: 10rpx
}

.article .bottom .title {
    font-size: 28rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    color: #333333;
    /* line-height: 2; */
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
}

.article .bottom .detail {
    font-size: 24rpx;
    color: #999999;
    margin-right: 10rpx;
    margin-top: 30rpx;
    line-height: 1;
}

.article .bottom .date {
    font-size: 24rpx;
    color: #999999;
    margin-right: 10rpx;
    margin-top: 30rpx;
    line-height: 1;
}
.article .bottom .detail .sale {
    text-decoration: line-through;
    font-size: 24rpx;
    font-weight: 400;
    color: #999999;
    margin-left: 10rpx;
}

.article .bottom .cart {
    width: 60rpx;
    height: 60rpx;
    position: absolute;
    bottom: 0rpx;
    right: 0rpx;
}

.article .bottom .cart image {
    width: 100%!important
}

.article .bottom .cart .num {
    position: absolute;
    top: 0rpx;
    right: 0rpx;
    width: 28rpx;
    height: 28rpx;
    border-radius: 50%;
    font-size: 20rpx;
    color: #fff;
    background: #CF2525;
    text-align: center;
    line-height: 28rpx
}


/* ljy 修改 width */

.article .bottom .user {
    width: 180rpx;
    height: 40rpx;
    border: 2rpx solid #C3E0E7;
    font-size: 20rpx;
    color: #A4CCD7;
    line-height: 40rpx;
    padding: 10rpx;
    border-radius: 20rpx;
}

.article .bottom .user image {
    width: 24rpx;
    height: 20rpx;
    margin-right: 2rpx;
}

.toButton::after {
    border: 0rpx!important;
}


/*重置小圆点*/


/*用来包裹所有的小圆点  */

.swiper {
    position: relative;
    background: rgba(249, 249, 249, 1);
    top: 0
}

.dots {
    display: flex;
    justify-content: space-between;
    flex-direction: row;
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
    bottom: 20rpx;
}


/*未选中时的小圆点样式 */

.dot {
    width: 12rpx;
    height: 12rpx;
    border-radius: 50%;
    margin-right: 10rpx;
    background-color: rgba(255, 255, 255, 0.5);
    overflow: hidden
}


/*选中以后的小圆点样式  */

.swiper .active {
    width: 30rpx;
    height: 12rpx;
    background-color: #ffffff;
    border-radius: 6rpx;
    position: relative;
    overflow: hidden
}
/* 首页商品 */


/* pages/culture/culture.wxss */

.tab-h {
    transform: translateY(-40rpx);
    padding-left: 30rpx;
    height: 100rpx;
    width: 100%;
    box-sizing: border-box;
    overflow: hidden;
    line-height: 80rpx;
    font-size: 28rpx;
    white-space: nowrap;
    z-index: 99;
    background: #fff
}

.tab-item {
    text-align: center;
    width:12%;
    margin: 0 36rpx;
    display: inline-block;
    color: rgba(153, 153, 153, 1);
}

.tab-item.active {
    color: rgba(51, 51, 51, 1);
    position: relative;
    font-weight: bold
}

.tab-item.active:after {
    content: "";
    display: block;
    height: 8rpx;
    border-radius: 4rpx;
    width: 40rpx;
    background: rgba(45, 135, 156, 1);
    position: absolute;
    bottom: 6rpx;
    left: 50%;
    transform: translateX(-50%);
    z-index: 101
}

.item-ans {
    width: 100%;
    display: flex;
    flex-grow: row no-wrap;
    justify-content: space-between;
    padding: 30rpx;
    box-sizing: border-box;
    height: 180rpx;
    align-items: center;
    border-bottom: 1px solid #F2F2F2;
}

.expertInfo {
    font-size: 12px;
    flex-grow: 2;
    color: #B0B0B0;
    line-height: 1.5em;
}

.expertInfo .name {
    font-size: 16px;
    color: #000;
    margin-bottom: 6px;
}

.askBtn {
    width: 120rpx;
    height: 60rpx;
    line-height: 60rpx;
    text-align: center;
    font-size: 14px;
    border-radius: 60rpx;
    border: 1px solid #4675F9;
    color: #4675F9;
}

.tab-content {
    margin-top: 30rpx;
}

.scoll-h {
    height: 100%;
}

.goods {
    width: 100%;
    position: relative;
    background-color: rgba(249, 249, 249, 1)
}

.goods .tabs .left{
    width: 60%;
    margin-left: 40rpx;
}

.goods .tabs{
    background: #ffffff;
}

.goods .van-dropdown-menu__item {
    justify-content: flex-start;
}

.goods image {
    width: 100%;
    height: 100%;
}

.goods .img {
    transform: translateY(-40rpx);
    z-index: 2000;
    position: absolute;
    top: 0rpx;
    right: 0rpx;
    width: 50rpx;
    height: 80rpx;
    background-image: linear-gradient(to right, rgba(255, 255, 255, 0), rgba(255, 255, 255, 1));
}

.goods .img image {
    width: 40rpx;
    height: 40rpx;
    float: right;
    position: absolute;
    top: 70rpx;
    right: 40rpx
}

.van-dropdown-item {
    top: 100rpx!important;
}

.advBox {
    height: 180rpx;
    padding: 30rpx;
    padding-top: 0;
    background-color: rgba(249, 249, 249, 1);
    transform: translateY(-40rpx);
}

.advBox .advImg {
    width: 330rpx;
    height: 150rpx;
    display: inline-block;
    margin-right: 30rpx;
}

.recommend {
    width: 690rpx;
    height: 400rpx;
    background: #E6AA4D;
    padding: 6rpx;
    margin-left: 30rpx;
    border-radius: 24rpx;
    transform: translateY(-70rpx);
}

.recommend .top {
    color: #ffffff;
    font-size: 28rpx;
    padding: 18rpx 24rpx;
}

.recommend .bottom {
    background: #ffffff;
    border-radius: 24rpx;
    padding: 20rpx 14rpx;
    height: 309rpx;
}


